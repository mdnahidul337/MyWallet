{
  "build": {
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./path-to-key.json",
        "track": "production"
      }
    }
  }
}